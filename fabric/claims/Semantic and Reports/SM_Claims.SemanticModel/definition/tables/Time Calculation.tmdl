table 'Time Calculation'
	lineageTag: de9a0f43-66c7-48f8-9224-ec8dfcffbea0

	calculationGroup

		calculationItem YTD = TOTALYTD(SELECTEDMEASURE(), Dates[Date])

		calculationItem QTD = TOTALQTD(SELECTEDMEASURE(), Dates[Date])

		calculationItem LY = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR(Dates[Date]))

		calculationItem MTD = TOTALMTD(SELECTEDMEASURE(), Dates[Date])

		calculationItem CY = SELECTEDMEASURE()

		calculationItem 'YoY Î”' = SELECTEDMEASURE() - CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR(Dates[Date]))

		calculationItem 'YoY %' =
				
				VAR Curr = SELECTEDMEASURE()
				VAR Prev = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR(Dates[Date]))
				RETURN DIVIDE(Curr - Prev, Prev)

		calculationItem 'Last 12M' = CALCULATE(SELECTEDMEASURE(), DATESINPERIOD(Dates[Date], MAX(Dates[Date]), -12, MONTH))

	column 'Time Calculation'
